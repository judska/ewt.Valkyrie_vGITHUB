{game_entity
  	(include "/properties/car.ext")
  	(include "/properties/selection/vehicle.inc"  scale(1.1))
  	{collider "truck"}
  	{props "vehicle" "car" "wheeled" "truck" "vision_vehicle_medium" "turret" "with_gun" "turn_over" "muzzle_medium" "gun_mode_change"}
	{PatherID  "vehicle.large"}
	("armor_medium" args 1.0)
  	("crew_2_human")
  	("board_animation"
		place(driver)
		board_begin(board_big-midcar)
		board_end(seat_driver_car)
		base(doorright)
	 )
	("board_animation"
		place(commander)
		board_begin(board_big-midcar_driver)
		board_end(seat_passenger)
		base(doorleft)
	)
  
	("board_animation" place(gunner)	board_begin(board_SdKfz251-1) board_end(cannon_m5_gunlayer) base(open))
	("board_animation" place(charger)	board_begin(board_SdKfz251-1) board_end(Cannon_pdr25_charger) base(open))
	("crew_human" place(gunner)	bone(gunner)	door(emit3) animation(gunner))
	("crew_human" place(charger)	bone(charger)	door(emit4) animation(charger))
	{placer
		{place "driver" 	{visor "driver"}	{group "crew"}}
		{place "commander"	{visor "view-around"}	{group "crew"}}
		{place "gunner"		{visor "main"}	{group "crew"} {openForCrush}}
		{place "charger"	{visor "charger"}	{group "crew"} {openForCrush}}
	}
	
	{sensor
		
		{visor "driver"
			{vision "human"}
			{bone "visor1"}
		}
		{visor "charger"
			{vision "human"}
			{bone "turret"}
		}
		{visor "main"
			{vision "cannon_main"}
			{bone "gun_rot"}
		}
	}
	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")
  
	{volume "gun" 
		{thickness 6}
	}
	{component "gun"
		{RepairTime 20}
	}
	{volume "turret"
		{thickness 10}
	}
	{component "turret"
		{RepairTime 20}
	}
	{volume "body"
		{tags "body"}
		{component "body"}
		{thickness 7}
	}
	{Volume "gunbed"
		{thickness 7}
		{able {nowounddecal}}
  	}
	
	
  	{mass 2000}
  	{Chassis
    	{Locomotion ("locomotion")
			{MaxSpeed	25}
      		{maxSpeedAtMaxTurn 12}
    	}
		{FuelBag
			{volume 124 }	
			{rate	5.2 }   
			{remain 124 }
			{fuel "fuel"}
		}
	("car_trace" fx("tracewheel02") step(0.09) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
	("car_trace" fx("tracewheel02") step(0.09) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
	}
  	{Extension "c50cma.mdl"}
	{extender "inventory"
    		{box
			{item "bullet75 ap" 40}
			{item "bullet75 fg" 40}
		}
	}
	{Weaponry
		{place "gun"
			{weapon "75mm_m75_27ck" filling "bullet75 ap" 1}
			{foresight "foresight1"}
			{gunner "gunner"}
			{charger "charger"}
			("abm_low")
		
		}
	}
	
}
