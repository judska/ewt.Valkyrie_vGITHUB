{game_entity
	(include "/properties/car.ext")
	(include "/properties/selection/vehicle.inc"  scale(1.1))
	{collider "truck"}
	{props "vehicle" "car" "truck" "wheeled" "vision_vehicle_high"}
	{able "emit_on_attack" "emit_passengers_on_enemy"}
	("armor_medium" args 1.0)
	{PatherID  "vehicle.large"}

	("crew_2_human")
	("board_animation"
		place(driver)
		board_begin(board_big-midcar)
		board_end(seat_driver_car)
		base(doorleft)
	)
	("board_animation"
		place(commander)
		board_begin(board_big-midcar_driver)
		board_end(seat_passenger)
		base(doorright)
	)
	
	("seat_inside")
	{boarder
		{anm "passenger"
			{forward	{begin	"board_body"} {end "seat_passenger"}}
			{reverse 	{end	"emit_body" 1}}
		}
	}
	("crew_human" place(seat1)	bone(seat01)	door(emit3) 	animation(passenger))
	("crew_human" place(seat2)	bone(seat02)	door(emit4)	animation(passenger))
	("crew_human" place(seat3)	bone(seat03)	door(emit3)	animation(passenger))
	("crew_human" place(seat4)	bone(seat04)	door(emit4)	animation(passenger))
	("crew_human" place(seat5)	bone(seat05)	door(emit3)	animation(passenger))
	("crew_human" place(seat6)	bone(seat06)	door(emit4)	animation(passenger))
	("crew_human" place(seat7)	bone(seat07)	door(emit3)	animation(passenger))
	("crew_human" place(seat8)	bone(seat08)	door(emit4)	animation(passenger))
	("crew_human" place(seat9)	bone(seat09)	door(emit3)	animation(passenger))
	
	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")
	("wheel" args "wheelright3")
	("wheel" args "wheelleft3")

	{volume "tarp"	
		{able {nowounddecal}}
		{thickness 1}
	}
	{component "tarp"	
		{RepairTime 10}
	}
	{volume "shield01"
		{able {nowounddecal}}
		{thickness 1}
	}
	{component "shield01"
		{RepairTime 15}
	}
	{volume "body"
		{tags "body"}
		{component "body"}
		{thickness 3}
	}
	{volume "body1"
		{tags "body"}
		{component "body"}
		{thickness 3}
	}
	{volume "cabin"
		{tags "cabin"}
		{component "cabin"}
		{thickness 3}
	}

	 {extender "inventory"
   		{box  
      			{size 8 12}
            			{clear}
			{item "hedgehog_kit" 7 {cell 0 0}}
			{item "hedgehog_kit" 7 {cell 0 2}}
			{item "sandbag_kit" 6 {cell 2 0}}
			{item "sandbag_kit" 6 {cell 2 2}}
			{item "sandbag_kit" 6 {cell 4 0}}
			{item "sandbag_kit" 6 {cell 4 2}}
			{item "barbwire_kit" 6 {cell 2 4}}
			{item "barbwire_kit" 6 {cell 2 6}}
			{item "barbwire_kit" 6 {cell 4 4}}
			{item "barbwire_kit" 6 {cell 4 6}}
			{item "antipersonnel" "mine" 30 {cell 0 4}}
			{item "antipersonnel" "mine" 30 {cell 0 5}}
			{item "antitank" "mine" 20 {cell 0 6}}
			{item "antitank" "mine" 20 {cell 0 8}}
			{item "dynamite" 5 {cell 2 8}}
			{item "repair_kit" {cell 4 8}}
		  	{item "mine_detector"}
                        {item "barrel" 2 filling "fuel" 500 {cell 0 7}}
		}
  	}
	
	{mass 4500}
	{Chassis
        {Locomotion ("locomotion")
			{MaxSpeed 38}
			{BrakeTime  2}  
			{StopTime   4.2}
        }
		{FuelBag
			{volume 100 }	
			{rate	3.4 }   
			{remain 100 }
			{fuel "fuel"}
		}
		("car_trace" fx("tracewheel01") step(0.09) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel01") step(0.09) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
	}
	{extender "shield"
			{on}
	}

	{Extension "fiat35.mdl"}
}
