{game_entity
	(include "/properties/tank.ext")
	(include "/properties/selection/vehicle.inc" scale(1.2))
	{props "medium" "zenite" "turret_rotator" "flak" "vision_vehicle_medium" "muzzle_minimum"}

	{extender "enumerator"}
	
	("armor_light" turret(25 25 12 8) body(25 15 7 8))
	("armor_mantlet" t(25))

	{boarder
		{anm "left"
			{forward	{begin	"board_tank_left"} {end "pose_vickers_gunlaeyr-L"}}
			{reverse 	{end	"emit_tank_1_hold"}}
		}
		{anm "right"
			{forward	{begin	"board_tank_right"} {end "pose_vickers_gunlaeyr-R"}}
			{reverse 	{end	"emit_tank_1_hold"}}
		}
		{anm "armor"
			{forward	{begin	"board_MiddleArmor"} {end "pose_seat_armor_1"}}
			{reverse 	{end	"emit_tank_2"}}
		}
		{anm "armor1"
			{forward	{begin	"board_MiddleArmor"} {end "pose_seat_armor_2"}}
			{reverse 	{end	"emit_body"}}
		}
	}

	("crew_human" 			place(driver)		door1(emit1) door2(emit2) visor(driver))
	("crew_human_visible"	place(gunner)		door1(emit2) door2(emit1) visor(main))
	("crew_human" 			place(commander)	door1(emit1) door2(emit2) visor(main))
	{placer 
		{place "gunner" {openForCrush 0}}
	}
	{sensor
		{visor "driver"
			{vision "tank_driver"}
			{bone "visor1"}
		}
	}

	("seat_human" place(seat1) bone(seat01) door(emit3)  animation(armor1))
	("seat_human" place(seat2) bone(seat02) door(emit3)  animation(armor))
	("seat_human" place(seat3) bone(seat03) door(emit4)  animation(armor1))
	("seat_human" place(seat4) bone(seat04) door(emit4)  animation(armor))
	("seat_human" place(seat5) bone(seat05) door(emit5)  animation(armor1))
	("seat_human" place(seat6) bone(seat06) door(emit6)  animation(armor))

	{extender "inventory"
		{box
			{item "bullet20 zenite" 900}
		}
	}
	{Weaponry
		("restore_ik_time")
		{place "gun"
			{RestoreIKAfterAim}
                        		{weapon "20mm_type2" filling "ammo bullet20 zenite" 10} 
                        		{gunner "gunner"}
			{charger "gunner"}
			("abm_zenite")
		}
	}
	{sensor
		{visor "main"
			{bone "visor1"}
		}
	}

	{mass 15400}
	{Chassis
		("tank_trace" fx("tracks_mid") step(0.56) len(0.14))
		{Locomotion ("locomotion")
			{MaxSpeed	26}
			{maxSpeedAtMaxTurn 13}
			{StartTime	 12}
			{TurnTime	8}
			{TurnStart  1.5} 
    		        {BrakeTime  1.5}
     		        {StopTime   2.5} 
    		        {TurnRadius 9} 
    		        {TurnStop   0.7} 
    		        {Gears      0.2 0.5 1.0}
    		        {RearGears  0.3}
		}
		{speed
        	{Normal  12}
        	{Slow  18}
    	}
		{FuelBag
			{volume 350 }	
			{rate	 90 }   
			{remain 350 }
			{fuel "fuel"}
		}
	}
	{Extension "ta-se.mdl"}
      	
	{Volume "body01"
		{able {holed}}
		{tags "body"}
		{component "body"}
	}
	{Volume "body02"
		{able {holed}}
		{tags "body"}
		{component "body"}
	}
	{Volume "body03"
		{tags "body"}
		{component "body"}
	}
	{Volume "body04"
		{tags "body"}
		{component "body"}
	}
	{Volume "body05"
		{tags "body"}
		{component "body"}
	}
	{Volume "body06"
		{tags "body"}
		{component "body"}
	}
	{Volume "body07"
		{tags "body"}
		{component "body"}
	}
	{Volume "engine01"
		{tags "engine"}
		{component "engine"}
	}
	{Volume "engine02"
		{tags "engine"}
		{component "engine"}
	}
	
	{Volume "trackleft"
	}
	{Volume "trackright"
	}
	{volume "turret1"
		{component "turret"}
		{tags "turret"}
	}
	{bone "turret"
		{speed 0.009}
	}
	
}
