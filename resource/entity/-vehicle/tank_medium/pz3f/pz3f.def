{game_entity
	(include "/properties/tank.ext")
	(include "/properties/selection/vehicle.inc"  scale(1.2))
	{props "medium" "turret_rotator" "vision_vehicle_high" "muzzle_small"}
	
	{extender "enumerator"
		{digit_folder "ger"}
	}

	{volume "body1"
		{tags "body"}
		{component "body"}
	}
	{volume "body2"
		{tags "body"}
		{component "body"}
	}
	{volume "body3"
		{tags "body"}
		{component "body"}
	}

	("armor_medium" turret(15 15 15 15) body(15 15 15 15))
	("armor_engine"  t(15 15 15 15))
	("armor_mantlet" t(15))

	{extender "shield"
		{on}
	}
	("crew_4_human")

	{boarder
		{anm "left"
			{forward	{begin	"board_tank_right"} {end "pose_mgun_pz6"}}
			{reverse 	{end	"emit_tank_2_hold"}}
		}
		{anm "right"
			{forward	{begin	"board_tank_left"} {end "pose_mgun_pz6"}}
			{reverse 	{end	"emit_tank_2_hold"}}
		}
		{anm "armor"
			{forward	{begin	"board_lowArmor"} 	{end "pose_seat_armor_2"}}
			{reverse 	{end	"emit_tank_2"}}
		}
		{anm "armor1"
			{forward	{begin	"board_lowArmor"} {end "pose_seat_armor_2"}}
			{reverse 	{end	"emit_body"}}
		}
		{anm "armor2"
			{forward	{begin	"board_lowArmor"} {end "pose_seat_armor_1"}}
			{reverse 	{end	"emit_body"}}
		}
	}

	("crew_human_visible" 	place(gunner2)	door1(emit1) door2(emit2) visor(main))

	("gunner_visible")

	("seat_human" place(seat1) bone(seat01) door(emit3)  animation(armor1))
	("seat_human" place(seat2) bone(seat02) door(emit3)  animation(armor2))
	("seat_human" place(seat3) bone(seat03) door(emit4)  animation(armor))
	("seat_human" place(seat4) bone(seat04) door(emit4)  animation(armor1))

	{extender "inventory"
		{box
			{item "ammo bullet50 ap" 50}
			{item "ammo bullet50 fg" 27}
			{item "ammo bullet50 apcr" 5}
			{item "ammo mgun" 650}
		}
	}
	{Weaponry
		("restore_ik_time")
		{place "gun"
			{RestoreIKAfterAim}
			{weapon "50mm_kwk38" filling "ammo bullet50 ap" 1}	
			("abm_medium")
		}
		{place "mgun1"
			{type "mgun_hull"}
			{weapon "mg34" filled}
			{foresight "foresight4"}
			{gunner "driver"}
			{charger "driver"}
			("abm_mgun")
		}
		{place "mgun2"
			{type "mgun_coaxial"}
			{weapon "mg34" filled}
			{foresight "foresight5"}
			("abm_mgun")
			{basic}
		}
	}

	{mass 28000}
	{Chassis
		("tank_trace" fx("tracks_mid") step(0.52) len(0.1))
		{Locomotion ("locomotion")
			{maxspeed 24}   
			{StartTime 7}   
			{TurnStart 1}   
			{TurnTime 8}    
			{RearGears 0.45}
		}		
		{FuelBag
			{volume 320 }	
			{rate	110 }   
			{remain 320 }
			{fuel "fuel"}
		}
		{speed
        	{Normal  10}
        	{Slow  16}
    	}
	}
	{Extension "pz3f.mdl"}
	{bone "turret"
		{speed 0.011}
	}
}
